---
import BaseHead from "../components/BaseHead.astro";
---

<!doctype html>
<html lang="en">
    <meta charset="utf-8" />
    <!-- <link rel="stylesheet" crossorigin="anonymous" href="https://assets.gumroad.com/packs/css/design-99355430.css" /> -->
    <style>
        .header {
            grid-template-columns: 1fr auto;
        }

        .logo-full {
            width: calc(1em / 0.14);
            -webkit-mask-image: url(data:image/svg+xml;base64,);
            mask-image: url(data:image/svg+xml;base64,);
            -webkit-mask-size: contain;
            mask-size: contain;
            background: #ffffff;
        }

        @media (min-width: 1024px) {
            body > header,
            header.sticky,
            main > * {
                padding: 4rem max((100% - 71.25rem)/2, 4rem);
            }

            body > header:where(.sidebar-nav *),
            header.sticky:where(.sidebar-nav *),
            main > *:where(.sidebar-nav *) {
                padding-left: 4rem;
                padding-right: max(100% - 71.25rem - 4rem, 4rem);
            }
        }

        .tab-buttons[role="tablist"] [role="tab"],
        .widget .overlay .close-button,
        .category-card,
        button,
        .button {
            background: transparent;
            font-size: 1rem;
            line-height: 1.5;
            --line-height: 1.5rem;
            padding: 0.75rem 1rem;
            border: solid 0.0625rem rgb(var(--color) / var(--border-alpha));
            color: currentColor;
            border-radius: 0.25rem;
            font-family: inherit;
            display: inline-flex;
            align-items: center;
            justify-content: center;
            gap: 0.5rem;
            cursor: pointer;
            -webkit-text-decoration: none;
            text-decoration: none;
            transition: var(--transition-duration) ease-out;
            transition-property: all;
        }

        .button-facebook {
            background-color: #4267b2;
            color: #fff;
        }

        .button-twitter {
            background-color: #4a99e9;
            color: #fff;
        }

        .button-paypal {
            background-color: #00457c;
            color: #fff;
        }

        .button-stripe {
            background-color: #625bf6;
            color: #fff;
        }

        .button-apple {
            background-color: #000;
            color: #fff;
        }

        .button-android {
            background-color: #142f40;
            color: #fff;
        }

        .button-kindle {
            background-color: #f3a642;
            color: #000;
        }

        .button-discord {
            background-color: #7289da;
            color: #fff;
        }

        .button-zoom {
            background-color: #4087fc;
            color: #fff;
        }

        .button-google {
            background-color: #5383ec;
            color: #fff;
        }

        input:not([type]),
        input[type="email"],
        input[type="date"],
        input[type="datetime-local"],
        input[type="number"],
        input[type="password"],
        input[type="search"],
        input[type="tel"],
        input[type="text"],
        input[type="url"],
        textarea,
        select,
        .input,
        .chosen-container .chosen-choices,
        .textarea {
            font-family: inherit;
            padding: 0.75rem 1rem;
            font-size: 1rem;
            line-height: 1.5;
            --line-height: 1.5rem;
            border: solid 0.0625rem rgb(var(--color) / var(--border-alpha));
            border-radius: 0.25rem;
            display: block;
            width: 100%;
            background-color: #000000;
            color: rgb(var(--contrast-filled));
            --active-bg: #242423;
        }
        [role=separator] {
    display: grid;
    grid-template-columns: 1fr auto 1fr;
    align-items: center;
    gap: .75rem
}

[role=separator]::before,[role=separator]::after {
    content: "";
    border-bottom: solid .0625rem rgb(var(--color)/var(--border-alpha))
}


@media(min-width: 1024px) {
    .modals-container aside {
        display:grid;
        position: relative
    }

    .modals-container aside>img {
        position: absolute;
        top: 0;
        left: 0;
        height: 100%;
        width: 100%;
        object-fit: cover
    }
}

@media(min-width: 1024px) {
    aside {
        border-left:solid .0625rem rgb(var(--color)/var(--border-alpha));
        grid-column: 2;
        width: 40vw;
        overflow: auto;
        background-color: rgb(var(--filled));
        color: rgb(var(--contrast-filled));
        --active-bg: var(--body-bg);
        padding: 1.5rem;
        gap: 1rem;
        grid-auto-rows: max-content;
        display: grid;
        position: fixed;
        top: 0;
        right: 0;
        bottom: 0;
        z-index: var(--z-index-menubar);
    }
}

    </style>
    <body class="bg-slate-600">
        <div class="modals-container" style="display: contents">
            <div class="modal gr-auth squished" style="display: contents">
                <main class="login-form">
                    <header class="header grid gap-4 grid-cols-2 pt-16">
                        <a class="wordmark logo-full" href="https://gumroad.com"
                        ></a>
                        <div class="text-base">
                            <a href="/signup?next=https%3A%2F%2Fgumroad.com%2F"
                                >注册</a
                            >
                        </div>
                        <h1 class="text-4xl">登录</h1>
                    </header>
                    <div class="input-group">
                        <section
                            style="display: grid; grid-gap: var(--spacer-6); padding-bottom: var(--spacer-7);"
                        >
                            <div
                                class="error-indicator danger"
                                role="alert"
                                style="display:none"
                            >
                            </div>
                            <div
                                style="grid-template-rows: 1fr"
                                class="gap-y-3 grid"
                            >
                                <form
                                    class="button_to"
                                    method="post"
                                    action="/users/auth/facebook?referer=https%3A%2F%2Fgumroad.com%2F"
                                >
                                    <button
                                        class="button button-facebook"
                                        style="width: 100%"
                                        >Facebook
                                    </button><input
                                        type="hidden"
                                        name="authenticity_token"
                                        value="Hd6tyjotVnCnd-NiIC6wUbBkkZTU-PR2yhxqcKxZ0DRzjQVQnQsT9JOGsqw5sEx7ze_JYTm9CfX-9m1CEO_KpQ"
                                        autocomplete="off"
                                    />
                                </form><form
                                    class="button_to"
                                    method="post"
                                    action="/users/auth/google_oauth2?referer=https%3A%2F%2Fgumroad.com%2F&x_auth_access_type=read"
                                >
                                    <button
                                        class="button button-google"
                                        style="width: 100%"
                                        >谷歌
                                    </button><input
                                        type="hidden"
                                        name="authenticity_token"
                                        value="lTU26olT2u1wOHZJCwolCCzZ_gb-2bDvXCtZUljRQvaPiVjklNplx3TZuay5KLtU7QU5LQnzZNOwZURBBHmRfA"
                                        autocomplete="off"
                                    />
                                </form><form
                                    class="button_to"
                                    method="post"
                                    action="/users/auth/twitter?referer=https%3A%2F%2Fgumroad.com%2F&x_auth_access_type=read"
                                >
                                    <button
                                        class="button button-twitter"
                                        style="width: 100%"
                                        ><span
                                            class="translation_missing"
                                            title="translation missing: zh-CN.twitter"
                                            >Twitter</span
                                        >
                                    </button><input
                                        type="hidden"
                                        name="authenticity_token"
                                        value="G-GXSQ-qXlbzk8tgnqvevzqn5dkhwMUfPqlG7W9s8uD5KbloEcXdmqqoMYRYiacrD6F7Gdb5zucB3qqwF2XpfQ"
                                        autocomplete="off"
                                    />
                                </form><form
                                    class="button_to"
                                    method="post"
                                    action="/users/auth/stripe_connect?referer=%2Flogin"
                                >
                                    <button
                                        class="button button-stripe"
                                        style="width: 100%"
                                        >条纹
                                    </button><input
                                        type="hidden"
                                        name="authenticity_token"
                                        value="6vANktPb-WVSUfWQLrkkFnYb32W6fOOkIXoQxtk42OJrMfOtEgJh8rApFv1pg4LnVpjA-5DG_r3MXHH0Nof0QA"
                                        autocomplete="off"
                                    />
                                </form>
                            </div>
                        </section>
                        <form
                            class="row stacked formtastic user"
                            id="login-form"
                            autocomplete="off"
                            novalidate="novalidate"
                            action="https://app.gumroad.com/login"
                            accept-charset="UTF-8"
                            method="post"
                        >
                            <input
                                type="hidden"
                                name="authenticity_token"
                                value="_B4F4mhDEHCaEtd2bhmwnp-IwwILCKd45SK1sW8yqVBV9bl6Ae8Rq6ksry_MNdsTkLPartaA8BEnAy4zyBBLEQ"
                                autocomplete="off"
                            /><section class="row classic-auth js-classic-auth">
                                <input
                                    type="hidden"
                                    name="next"
                                    id="next"
                                    value="https://gumroad.com/"
                                    autocomplete="off"
                                />
                                <div class="divider" role="separator">
                                    <span>或者</span>
                                </div>
                                <fieldset>
                                    <legend>
                                        <label class="top-level-label"
                                            >电子邮件</label
                                        >
                                        <a
                                            class="legacy-only pull-right"
                                            href="/signup?next=https%3A%2F%2Fgumroad.com%2F"
                                            >想要创建一个帐户？</a
                                        >
                                    </legend>
                                    <input
                                        class="required top-level-input"
                                        tabindex="1"
                                        type="email"
                                        name="user[login_identifier]"
                                    />
                                </fieldset>
                                <fieldset>
                                    <legend>
                                        <label class="top-level-label"
                                            >密码</label
                                        >
                                        <a
                                            class="pull-right js-show-forgot-password-form-trigger"
                                            href="#">忘记密码了吗？</a
                                        >
                                    </legend>
                                    <input
                                        class="password required top-level-input"
                                        tabindex="2"
                                        type="password"
                                        name="user[password]"
                                    />
                                </fieldset>
                                <button
                                    class="button-primary button-block button-full-width primary p-4 bg-black"
                                    >登录</button
                                >
                                <div
                                    class="js-recaptcha"
                                    data-sitekey="6LfD8OcaAAAAAPeGM03M5l8k1UXcAblGS0vS9uVU"
                                    id="loginRecaptcha"
                                >
                                </div>
                            </section>
                        </form><form
                            id="forgot-password-form"
                            class="soft-hidden formtastic user"
                            novalidate="novalidate"
                            action="https://app.gumroad.com/forgot_password"
                            accept-charset="UTF-8"
                            method="post"
                        >
                            <input
                                type="hidden"
                                name="authenticity_token"
                                value="ShSd9jp2ZIcQ4Heo4qWNW8Ru9ndPaOmwiIzNS6LLScgFNTc2wE20Q3NkecD0SCBi2un9K14cKxIXS4WotpKMIA"
                                autocomplete="off"
                            /><section>
                                <div
                                    class="error-indicator danger"
                                    role="alert"
                                    style="display:none"
                                >
                                </div>
                                <fieldset>
                                    <legend>
                                        <label class="top-level-label"
                                            >通过电子邮件将重置说明发送至</label
                                        >
                                    </legend>
                                    <input
                                        class="email top-level-input required"
                                        type="email"
                                        name="user[email]"
                                    />
                                </fieldset>
                                <button
                                    class="primary button-primary button-block button-full-width js-forgot-password-button"
                                    >发送</button
                                >
                                <a
                                    class="button button-default button-block button-full-width js-hide-forgot-password-form-trigger"
                                    href="#">取消</a
                                >
                            </section>
                        </form>
                    </div>
                </main>
            </div>
            <aside class="avatars-container">
                <img
                    src="https://assets.gumroad.com/assets/auth/background-29d487bd6d844e8a6b763d36a7bff15523c665c4e44f8fd67840815f2edbea24.png"
                />
            </aside>
        </div>
    </body>
</html>
